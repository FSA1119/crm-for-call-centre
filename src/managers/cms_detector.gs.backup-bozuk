/**
 * CMS / E-Ticaret Platform Tespiti â€“ SatÄ±ÅŸ OdaklÄ±
 * Version: 4.4
 * Ã‡Ä±ktÄ± kurallarÄ±:
 *  - Bilinen platform: sadece CMS adÄ±
 *  - Pazar yeri/sosyal maÄŸaza alan adÄ±: "Sitesi Yok"
 *  - Bilinmeyen (custom): kalite analizine gÃ¶re "DÃ¼ÅŸÃ¼k Kalite" | "YÃ¼ksek Kalite"
 */

function cmsDetectFunctionStarted(parameters) {
  console.log('Function started:', parameters);
}

/**
 * Ana giriÅŸ â€“ tek URL iÃ§in tespit
 * @param {string} rawUrl URL veya domain
 * @returns {string} SonuÃ§ (Ã¶r: "Shopify" | "WooCommerce" | "Sitesi Yok" | "DÃ¼ÅŸÃ¼k Kalite" | "YÃ¼ksek Kalite")
 */
function detectCMSForUrl(rawUrl) {
  cmsDetectFunctionStarted({ rawUrl });
  try {
    const url = normalizeUrl(rawUrl);
    if (!url) return '';

    // âš ï¸ KRÄ°TÄ°K: Ä°LK Ã–NCE domain kontrolÃ¼ (fetch'ten Ã–NCE!)
    const domain = extractDomain(url);
    console.log('ğŸ” Domain Ã§Ä±karÄ±ldÄ±:', { url, domain });
    
    if (isMarketplaceOrSocial(domain)) {
      console.log('ğŸ›‘ Pazar yeri/sosyal medya tespit edildi:', domain);
      return 'Sitesi Yok';
    }

    const fetched = fetchSiteWithRedirects(url);
    if (fetched && fetched.unreachable) {
      console.log('â›” Site eriÅŸilemez:', url);
      return 'EriÅŸilemedi';
    }
    const signals = buildSignals(fetched);
    console.log('ğŸ“Š Signals oluÅŸturuldu:', { domain: signals.domain, htmlLength: signals.html.length });

    let known = detectKnownCMS(signals);
    if (known) {
      // EÄŸer Shopify ve site kapalÄ± ise (4xx/5xx veya body'de offline ibareleri), EriÅŸilemedi yaz
      if (known === 'Shopify') {
        const offlineHint = /(store\s+is\s+unavailable|we\'re\s+currently\s+unavailable|site\s+is\s+down|coming\s+soon|password\s+page)/i;
        if ((signals.statusCode && signals.statusCode >= 400) || offlineHint.test(signals.html)) {
          return 'EriÅŸilemedi';
        }
      }
      console.log('Known CMS matched:', known, { url: fetched.finalUrl });
      return known;
    }

    // Deep probe (accurate mode fallback)
    known = deepProbeCMS(fetched.finalUrl);
    if (known) {
      console.log('Deep probe matched:', known, { url: fetched.finalUrl });
      return known;
    }

    // E-ticaret sinyali varsa ama CMS bulunamadÄ±ysa, Ã¶nce kalite kontrol et
    const hasEcommerceSignals = /sepet|cart|basket|Ã¼ye ol|giriÅŸ yap|Ã¶deme|payment|checkout|kategori|Ã¼rÃ¼n|product|add to cart|sepete ekle/i.test(signals.html);
    
    if (hasEcommerceSignals) {
      // E-ticaret sitesi ama CMS tespit edilemedi - kalite bazlÄ± karar
      const quality = classifyCustomQuality(signals);
      
      // YÃ¼ksek kaliteli custom e-ticaret -> Ã¶zel sistem olarak iÅŸaretle
      if (quality === 'YÃ¼ksek Kalite') {
        console.log('High quality custom e-commerce detected:', { url: fetched.finalUrl });
        return 'Ã–zel E-ticaret'; // Custom ama kaliteli
      }
      
      // DÃ¼ÅŸÃ¼k/orta kalite -> tespit edilemedi
      console.log('E-commerce signals found but CMS unknown:', quality, { url: fetched.finalUrl });
      return quality; // "Orta Kalite" | "Normal Kalite" | "DÃ¼ÅŸÃ¼k Kalite"
    }

    // E-ticaret deÄŸil, normal site - kalite analizi
    const quality = classifyCustomQuality(signals);
    console.log('Custom build classified:', quality, { url: fetched.finalUrl });
    return quality;
  } catch (error) {
    console.error('Function failed:', error);
    try { SpreadsheetApp.getUi().alert('Hata', String(error && error.message || error), SpreadsheetApp.getUi().ButtonSet.OK); } catch(_) {}
    // Hata durumunda algoritmik tespit baÅŸarÄ±sÄ±zlÄ±ÄŸÄ± olarak dÃ¶n
    return 'Tespit Edilemedi';
  }
}

function deepProbeCMS(finalUrl) {
  try {
    if (!finalUrl) return '';
    var base = '';
    try { base = (new (this.URL || Url)(finalUrl)).origin; } catch(_) { base = finalUrl.replace(/^(https?:\/\/[^\/]+).*/, '$1'); }
    const probes = [
      // Shopify tespit endpoint'leri
      { name: 'Shopify', url: base + '/cart.js', type: 'json', mustContain: 'items' },
      { name: 'Shopify', url: base + '/collections.json', type: 'json' },
      { name: 'Shopify', url: base + '/products.json', type: 'json' },
      { name: 'Shopify', url: base + '/admin', type: 'text', mustContain: 'Shopify' },
      // WordPress & WooCommerce tespit endpoint'leri
      { name: 'WordPress', url: base + '/wp-json/', type: 'json' },
      { name: 'WordPress', url: base + '/wp-login.php', type: 'text', mustContain: 'WordPress' },
      { name: 'WordPress', url: base + '/wp-admin/', type: 'text', mustContain: 'WordPress' },
      { name: 'WooCommerce', url: base + '/wp-content/plugins/woocommerce/readme.txt', type: 'text', mustContain: 'WooCommerce' },
      { name: 'WooCommerce', url: base + '/?wc-ajax=cart', type: 'json' },
      { name: 'WooCommerce', url: base + '/wp-json/wc/v3/', type: 'json' },
      // Magento tespit endpoint'leri
      { name: 'Magento', url: base + '/rest/V1/', type: 'json' },
      { name: 'Magento', url: base + '/admin', type: 'text', mustContain: 'Magento' },
      // IdeaSoft tespit endpoint'leri (geniÅŸletilmiÅŸ)
      { name: 'IdeaSoft', url: base + '/admin', type: 'text', mustContain: 'IdeaSoft' },
      { name: 'IdeaSoft', url: base + '/admin/login', type: 'text', mustContain: 'IdeaSoft' },
      { name: 'IdeaSoft', url: base + '/Admin', type: 'text', mustContain: 'IdeaSoft' },
      { name: 'IdeaSoft', url: base + '/Admin/Login', type: 'text', mustContain: 'IdeaSoft' },
      { name: 'IdeaSoft', url: base + '/api/', type: 'json' },
      { name: 'IdeaSoft', url: base + '/api/cart', type: 'json' },
      { name: 'IdeaSoft', url: base + '/api/products', type: 'json' },
      { name: 'IdeaSoft', url: base + '/Service/', type: 'json' },
      { name: 'IdeaSoft', url: base + '/service/product', type: 'json' },
      // Ticimax tespit endpoint'leri
      { name: 'Ticimax', url: base + '/admin', type: 'text', mustContain: 'Ticimax' },
      { name: 'Ticimax', url: base + '/admin/login', type: 'text', mustContain: 'Ticimax' },
      { name: 'Ticimax', url: base + '/api/cart', type: 'json' },
      { name: 'Ticimax', url: base + '/api/products', type: 'json' },
      // T-Soft tespit endpoint'leri
      { name: 'T-Soft', url: base + '/admin', type: 'text', mustContain: 'T-Soft' },
      { name: 'T-Soft', url: base + '/admin/login', type: 'text', mustContain: 'TEKROM' },
      // Ä°kas tespit endpoint'leri
      { name: 'Ä°kas', url: base + '/admin', type: 'text', mustContain: 'ikas' },
      { name: 'Ä°kas', url: base + '/api/', type: 'json' }
    ];
    const reqs = probes.map(p => ({ url: p.url, muteHttpExceptions: true, followRedirects: true, headers: { 'User-Agent': 'Mozilla/5.0 (LeadBot; CRM-Detector)' } }));
    const resps = UrlFetchApp.fetchAll(reqs);
    for (let i = 0; i < resps.length; i++) {
      try {
        const ct = String(resps[i].getHeaders()['Content-Type'] || '').toLowerCase();
        const body = resps[i].getContentText();
        const p = probes[i];
        if (p.type === 'json' && /json/.test(ct)) return p.name;
        if (p.type === 'text') {
          if (!p.mustContain || body.indexOf(p.mustContain) !== -1) return p.name;
        }
      } catch(_) {}
    }
    return '';
  } catch (_) { return ''; }
}

/**
 * Ã‡oklu URL iÃ§in toplu tespit
 * @param {string[]} urls
 * @returns {string[]} sonuÃ§lar sÄ±rayla
 */
function detectCMSBatch(urls) {
  cmsDetectFunctionStarted({ size: urls && urls.length });
  try {
    if (!Array.isArray(urls) || urls.length === 0) return [];
    const out = [];
    for (const u of urls) {
      out.push(detectCMSForUrl(u));
    }
    console.log('Processing complete:', { size: out.length });
    return out;
  } catch (error) {
    console.error('Function failed:', error);
    return urls.map(_ => 'Tespit Edilemedi');
  }
}

/**
 * Paralel hÄ±zlÄ± toplu tespit (redirect takibi yok; 1 dakikalÄ±k toplu taramalar iÃ§in)
 */
function detectCMSBatchFast(urls) {
  cmsDetectFunctionStarted({ fast: true, size: urls && urls.length });
  try {
    if (!Array.isArray(urls) || urls.length === 0) return [];
    
    // âš ï¸ KRÄ°TÄ°K: Ä°LK Ã–NCE tÃ¼m URL'lerin domain'lerini kontrol et!
    const urlData = urls.map(rawUrl => {
      const url = normalizeUrl(rawUrl);
      const domain = extractDomain(url);
      return { rawUrl, url, domain };
    });
    
    // Pazar yeri/sosyal medya olanlarÄ± hemen ayÄ±r
    const out = [];
    const needsFetch = [];
    
    for (let i = 0; i < urlData.length; i++) {
      if (isMarketplaceOrSocial(urlData[i].domain)) {
        console.log(`ğŸ›‘ Pazar yeri/sosyal: ${urlData[i].domain}`);
        out[i] = 'Sitesi Yok';
      } else {
        out[i] = null; // HenÃ¼z tespit edilmedi
        needsFetch.push({ index: i, url: urlData[i].url });
      }
    }
    
    // Sadece gerÃ§ek siteleri fetch et
    if (needsFetch.length === 0) return out;
    
    console.log(`ğŸ“¡ ${needsFetch.length}/${urls.length} site fetch edilecek`);
    const reqs = needsFetch.map(item => ({ url: item.url, muteHttpExceptions: true, followRedirects: true, headers: { 'User-Agent': 'Mozilla/5.0 (LeadBot; CRM-Detector)' } }));
    const resps = UrlFetchApp.fetchAll(reqs);
    
    for (let i = 0; i < resps.length; i++) {
      try {
        const html = resps[i].getContentText();
        const headers = normalizeHeaders(resps[i].getAllHeaders());
        const finalUrl = needsFetch[i].url;
        const signals = { html, headers, finalUrl, domain: extractDomain(finalUrl) };
        const known = detectKnownCMS(signals);
        out[needsFetch[i].index] = known || classifyCustomQuality(signals);
      } catch (e) {
        out[needsFetch[i].index] = 'Tespit Edilemedi';
      }
    }
    
    return out;
  } catch (error) {
    console.error('Function failed:', error);
    return urls.map(_ => 'Tespit Edilemedi');
  }
}

function mapCmsGroup(name) {
  const n = String(name||'').toLowerCase();
  if (!n) return '';
  
  // Kalite kategorileri
  if (n.indexOf('yÃ¼ksek kalite') !== -1) return 'YÃ¼ksek Kalite';
  if (n.indexOf('normal kalite') !== -1) return 'Normal Kalite';
  if (n.indexOf('orta kalite') !== -1) return 'Orta Kalite';
  if (n.indexOf('dÃ¼ÅŸÃ¼k kalite') !== -1) return 'DÃ¼ÅŸÃ¼k Kalite';
  if (n.indexOf('Ã¶zel e-ticaret') !== -1) return 'Ã–zel E-ticaret';
  
  // Ã–zel durumlar
  if (n.indexOf('sitesi yok') !== -1) return 'Sitesi Yok';
  if (n.indexOf('eriÅŸilemedi') !== -1) return 'EriÅŸilemedi';
  if (n.indexOf('tespit edilemedi') !== -1) return 'Tespit Edilemedi';
  
  // Platform gruplarÄ±
  if (['ticimax','t-soft','ikas'].some(k=>n.indexOf(k)!==-1)) return 'Rakip';
  if (['ideasoft','eticaret.com'].some(k=>n.indexOf(k)!==-1)) return 'Bizim';
  if (['shopify','wix','bigcommerce','squarespace','ecwid','webflow','weebly','volusion','shift4shop','bigcartel','selz','godaddy'].some(k=>n.indexOf(k)!==-1)) return 'SaaS';
  if (['wordpress','woocommerce','magento','prestashop','opencart','shopware','oscommerce','zencart','nopcommerce','bagisto','saleor','sylius','spree'].some(k=>n.indexOf(k)!==-1)) return 'AÃ§Ä±k Kaynak';
  if (['ticimax','t-soft','ikas','ideasoft','eticaret.com','platinmarket','projesoft','faprika','neticaret','eticaretsoft','smart e-ticaret','shopphp','softtr','demresa','bilgikurumsal','quka','imagaza','akinsoft','hipotenus','ticifly','alkissoft','kobimaster','vatansoft','inplato','eticaretim','jettycart','dogruajans'].some(k=>n.indexOf(k)!==-1)) return 'TR HazÄ±r';
  
  return 'Custom';
}

function getCmsDefaults() {
  try {
    if (typeof CRM_CONFIG !== 'undefined' && CRM_CONFIG && CRM_CONFIG.CMS_DETECTOR) {
      return CRM_CONFIG.CMS_DETECTOR;
    }
  } catch(_) {}
  return {
    DEFAULT_CHUNK_SIZE: 100,
    MAX_CHUNK_SIZE: 200,
    DEFAULT_LIMIT_ROWS: 100,
    TARGET_SHEETS: ['RandevularÄ±m','Randevular','FÄ±rsatlarÄ±m','FÄ±rsatlar','ToplantÄ±larÄ±m','ToplantÄ±lar'],
    WEBSITE_COLUMN: 'Website',
    CMS_NAME_COLUMN: 'CMS AdÄ±',
    CMS_GROUP_COLUMN: 'CMS Grubu',
    // Alternatif kolon isimleri (TÃ¼rkÃ§e karakter varyasyonlarÄ± dahil!)
    ALT_WEBSITE_COLUMNS: ['Website', 'Web Site', 'website', 'Site', 'URL', 'Web Sitesi', 'WEBSITE', 'WebSite'],
    ALT_CMS_NAME_COLUMNS: ['CMS AdÄ±', 'CMS Adi', 'CMS ADI', 'CMS AdI', 'CMS', 'Alt YapÄ±', 'AltyapÄ±', 'Alt YapI', 'AltYapi'],
    ALT_CMS_GROUP_COLUMNS: ['CMS Grubu', 'CMS Grubu', 'CMS Group', 'CMS GRUBU', 'CMS GrubU', 'Grup', 'Platform Grubu', 'GRUP']
  };
}

/**
 * ğŸ” Esnek kolon bulma - alternatif isimleri de kontrol eder
 * TÃ¼rkÃ§e karakter sorunlarÄ±nÄ± Ã§Ã¶zer (Ä±/i, Ä°/I, ÅŸ/s, ÄŸ/g, Ã¼/u, Ã¶/o, Ã§/c)
 */
function findColumnIndex(headers, preferredName, alternatives) {
  // TÃ¼rkÃ§e karakterleri normalize et
  const normalizeTurkish = (str) => {
    return String(str)
      .toLowerCase()
      .replace(/Ä±/g, 'i')
      .replace(/ÄŸ/g, 'g')
      .replace(/Ã¼/g, 'u')
      .replace(/ÅŸ/g, 's')
      .replace(/Ã¶/g, 'o')
      .replace(/Ã§/g, 'c')
      .trim();
  };
  
  // Ã–nce exact match (tam eÅŸleÅŸme)
  let idx = headers.indexOf(preferredName);
  if (idx !== -1) return idx;
  
  // Alternatif isimleri ara (exact)
  if (alternatives && Array.isArray(alternatives)) {
    for (const alt of alternatives) {
      idx = headers.indexOf(alt);
      if (idx !== -1) return idx;
    }
  }
  
  // TÃ¼rkÃ§e karakterleri normalize ederek ara
  const normalizedPreferred = normalizeTurkish(preferredName);
  
  for (let i = 0; i < headers.length; i++) {
    const normalizedHeader = normalizeTurkish(headers[i]);
    
    // Tercih edilen isim ile karÅŸÄ±laÅŸtÄ±r
    if (normalizedHeader === normalizedPreferred) return i;
    
    // Alternatifler ile karÅŸÄ±laÅŸtÄ±r
    if (alternatives && Array.isArray(alternatives)) {
      for (const alt of alternatives) {
        if (normalizedHeader === normalizeTurkish(alt)) return i;
      }
    }
  }
  
  return -1;
}

/**
 * Aktif (temsilci) dosyada toplu CMS taramasÄ±
 * options: { chunkSize?: number, limitRows?: number, sheetNames?: string[] }
 */
function runCMSDetectionForCurrentAgent(options) {
  cmsDetectFunctionStarted({ scope: 'current-agent', options });
  const DEF = getCmsDefaults();
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const chunkSize = Math.max(10, Math.min(DEF.MAX_CHUNK_SIZE, (options && options.chunkSize) || DEF.DEFAULT_CHUNK_SIZE));
    const limitRows = options && options.limitRows ? Number(options.limitRows) : (options && options.limitRows === 0 ? 0 : DEF.DEFAULT_LIMIT_ROWS);
    const targetSheets = (options && options.sheetNames) || DEF.TARGET_SHEETS;

    let total = 0, updated = 0;
    for (const name of targetSheets) {
      const sh = ss.getSheetByName(name);
      if (!sh) continue;
      const lastRow = sh.getLastRow();
      if (lastRow <= 1) continue;
      const headers = sh.getRange(1,1,1,sh.getLastColumn()).getDisplayValues()[0];
      
      // Esnek kolon bulma
      const iWebsite = findColumnIndex(headers, DEF.WEBSITE_COLUMN, DEF.ALT_WEBSITE_COLUMNS);
      if (iWebsite === -1) {
        console.log(`â­ï¸ ${name} sayfasÄ± atlandÄ±: Website kolonu yok`);
        continue;
      }
      
      let iCms = findColumnIndex(headers, DEF.CMS_NAME_COLUMN, DEF.ALT_CMS_NAME_COLUMNS);
      let iGroup = findColumnIndex(headers, DEF.CMS_GROUP_COLUMN, DEF.ALT_CMS_GROUP_COLUMNS);
      
      // CMS kolonlarÄ± yoksa otomatik ekle
      if (iCms === -1 || iGroup === -1) {
        let insertPos = iWebsite + 1;
        
        if (iCms === -1) {
          sh.insertColumnAfter(iWebsite);
          sh.getRange(1, insertPos + 1).setValue('CMS AdÄ±');
          console.log(`âœ… '${name}' sayfasÄ±na 'CMS AdÄ±' kolonu eklendi`);
          insertPos++;
        }
        
        if (iGroup === -1) {
          sh.insertColumnAfter(insertPos);
          sh.getRange(1, insertPos + 1).setValue('CMS Grubu');
          console.log(`âœ… '${name}' sayfasÄ±na 'CMS Grubu' kolonu eklendi`);
        }
        
        // KolonlarÄ± yeniden bul
        const newHeaders = sh.getRange(1, 1, 1, sh.getLastColumn()).getDisplayValues()[0];
        iCms = findColumnIndex(newHeaders, DEF.CMS_NAME_COLUMN, DEF.ALT_CMS_NAME_COLUMNS);
        iGroup = findColumnIndex(newHeaders, DEF.CMS_GROUP_COLUMN, DEF.ALT_CMS_GROUP_COLUMNS);
      }

      const rowsCount = limitRows>0 ? Math.min(limitRows, lastRow-1) : (lastRow-1);
      const urls = sh.getRange(2, iWebsite+1, rowsCount, 1).getDisplayValues().map(r=>String(r[0]||''));
      total += urls.length;

      for (let start=0; start<urls.length; start+=chunkSize) {
        const part = urls.slice(start, start+chunkSize);
        const res = detectCMSBatchFast(part);
        for (let j=0; j<res.length; j++) {
          const rowIndex = 2 + start + j;
          const cmsName = res[j] || '';
          if (iCms !== -1) sh.getRange(rowIndex, iCms+1).setValue(cmsName);
          if (iGroup !== -1) sh.getRange(rowIndex, iGroup+1).setValue(mapCmsGroup(cmsName));
          updated++;
        }
      }
    }
    SpreadsheetApp.getUi().alert('CMS Analizi', `${ss.getName()} â†’ ${updated}/${total} satÄ±r iÅŸlendi`, SpreadsheetApp.getUi().ButtonSet.OK);
    return { total, updated };
  } catch (error) {
    console.error('Function failed:', error);
    SpreadsheetApp.getUi().alert('Hata', String(error && error.message || error), SpreadsheetApp.getUi().ButtonSet.OK);
    throw error;
  }
}

function openCMSDetectionCurrentAgentFast() {
  return runCMSDetectionForCurrentAgent({ chunkSize: getCmsDefaults().DEFAULT_CHUNK_SIZE, limitRows: getCmsDefaults().DEFAULT_LIMIT_ROWS, sheetNames: getCmsDefaults().TARGET_SHEETS });
}

function openCMSDetectionCurrentAgentAll() {
  return runCMSDetectionForCurrentAgent({ chunkSize: getCmsDefaults().DEFAULT_CHUNK_SIZE, sheetNames: getCmsDefaults().TARGET_SHEETS, limitRows: 0 });
}

