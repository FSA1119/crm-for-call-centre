function detectKnownCMS(signals) {
  const { html, headers, finalUrl } = signals;
  const lowHtml = String(html || '').toLowerCase();
  const headerPairs = Object.entries(headers || {});

  // 0) Meta generator (geniÅŸletilmiÅŸ)
  const genMatch = /<meta[^>]+name=["']generator["'][^>]+content=["']([^"']+)["']/i.exec(html || '') || /<meta[^>]+content=["']([^"']+)["'][^>]+name=["']generator["']/i.exec(html || '');
      if (genMatch && genMatch[1]) {
      const g = genMatch[1].toLowerCase();
      // E-ticaret platformlarÄ±
      if (/wordpress|woocommerce/.test(g)) return /woocommerce/.test(g) ? 'WooCommerce' : 'WordPress';
      if (/shopify/.test(g)) return 'Shopify';
      if (/wix/.test(g)) return 'Wix';
      if (/squarespace/.test(g)) return 'Squarespace';
      if (/prestashop/.test(g)) return 'PrestaShop';
      if (/opencart/.test(g)) return 'OpenCart';
      if (/magento/.test(g)) return 'Magento';
      if (/shopware/.test(g)) return 'Shopware';
      if (/bigcommerce/.test(g)) return 'BigCommerce';
      if (/ecwid/.test(g)) return 'Ecwid';
      if (/webflow/.test(g)) return 'Webflow';
      // TR platformlarÄ±
      if (/t-?soft/.test(g)) return 'T-Soft';
      if (/ticimax/.test(g)) return 'Ticimax';
      if (/ikas/.test(g)) return 'Ä°kas';
      if (/ideasoft|ak[Ä±i]ll[Ä±i]\s*e[-\s]?ticaret|ideasoft\s*e[-\s]?ticaret|ideasoft\s*sistemi/.test(g)) return 'IdeaSoft';
      if (/platinmarket/.test(g)) return 'PlatinMarket';
      if (/eticaret\.com/.test(g)) return 'Eticaret.com';
      if (/projesoft/.test(g)) return 'Projesoft';
    }

  // 1) Ã–NCE: Ana domain kontrolÃ¼ (en gÃ¼venilir!)
  try {
    const u = new URL(finalUrl);
    const host = u.hostname.toLowerCase();
    
    // Ana domain eÅŸleÅŸmesi - kesin tespit
    for (const s of CMS_SIGNATURES) {
      if (s.assets) {
        for (const re of s.assets) {
          if (re.test(host)) {
            console.log(`ğŸ¯ Ana domain tespit: ${s.name} (host: ${host})`);
            return s.name;
          }
        }
      }
    }
  } catch(_) {}
  
  // 2) CDN URL'leri HTML iÃ§inde (Ã¶rn: src="https://ideacdn.net/...")
  // Ã–ncelik sÄ±rasÄ±: Ticimax > T-Soft > Ä°kas > IdeaSoft (Ã§akÄ±ÅŸma Ã¶nleme)
  const cdnPriority = [
    { name: 'Ticimax', patterns: [/ticimaxcdn/i, /ticimaxstatic/i, /ticimaxassets/i, /cdn\.ticimax/i] },
    { name: 'T-Soft', patterns: [/tsoftcdn/i, /tsoftstatic/i, /cdn\.tsoft/i] },
    { name: 'Ä°kas', patterns: [/ikascdn/i, /ikasstatic/i, /ikasassets/i, /cdn\.ikas/i] },
    { name: 'IdeaSoft', patterns: [/ideacdn\.net/i, /ideacdn\.com/i, /ideasoftcdn/i, /cdn\.ideasofthosting/i, /\/\/ideacdn\./i, /\/\/cdn\.ideasoft/i] }
  ];
  
  for (const platform of cdnPriority) {
    for (const pattern of platform.patterns) {
      if (pattern.test(lowHtml)) {
        console.log(`ğŸ¯ CDN tespit: ${platform.name} (pattern: ${pattern})`);
        return platform.name;
      }
    }
  }
  
  // 3) HTML gÃ¶vde parmak izleri (dikkatli!)
  for (const s of CMS_SIGNATURES) {
    if (s.html) {
      for (const re of s.html) { 
        if (re.test(html)) {
          console.log(`ğŸ“ HTML pattern tespit: ${s.name} (pattern: ${re})`);
          return s.name; 
        }
      }
    }
  }
  // 3) Header parmak izleri ve cookie adlarÄ±
  const headersJoined = Object.entries(headers||{}).map(([k,v]) => `${k}: ${v}`).join('\n');
  for (const s of CMS_SIGNATURES) {
    if (s.headers) {
      for (const re of s.headers) {
        if (re.test(headersJoined)) return s.name;
      }
    }
  }
  return '';
}

// ----------------- Kalite Analizi (Custom) -----------------

function classifyCustomQuality(signals) {
  const { html, headers, finalUrl } = signals;
  let score = 0;
  
  // âš¡ HÄ±z optimizasyonu: lowerHtml'i sadece gerektiÄŸinde hesapla
  let lowerHtml = null;
  const getLowerHtml = () => {
    if (lowerHtml === null) lowerHtml = html.toLowerCase();
    return lowerHtml;
  };

  // âš¡ ERKEN Ã‡IKIÅ: Ã‡ok kÃ¶tÃ¼ siteleri hemen tespit et
  if (html.length < 500) return 'DÃ¼ÅŸÃ¼k Kalite'; // Ã‡ok az iÃ§erik
  
  const isHttps = /^https:/i.test(finalUrl);
  if (!isHttps && html.length < 2000) {
    // HTTP + az iÃ§erik = dÃ¼ÅŸÃ¼k kalite
    return 'DÃ¼ÅŸÃ¼k Kalite';
  }

  // ğŸ”’ GÃ¼venlik & temel hijyen (aÄŸÄ±rlÄ±klÄ±)
  if (isHttps) score += 3; else score -= 3; // HTTPS Ã§ok Ã¶nemli
  if (/content-security-policy/i.test(headers['content-security-policy'] || '')) score += 2;
  if (/strict-transport-security/i.test(headers['strict-transport-security'] || '')) score += 2;
  if (/x-frame-options/i.test(headers['x-frame-options'] || '')) score += 1;

  // ğŸ¨ SEO / semantik (iyileÅŸtirilmiÅŸ)
  const titleMatch = /<title>([^<]+)<\/title>/i.exec(html);
  if (titleMatch && titleMatch[1].length > 10) score += 2; else if (titleMatch) score += 1;
  if (/<meta[^>]+name=["']description["'][^>]+content=["'][^"']{20,}/i.test(html)) score += 2;
  if (/<link[^>]+rel=["']canonical["']/i.test(html)) score += 1;
  if (/<meta[^>]+property=["']og:title["']/i.test(html)) score += 1; // OpenGraph
  if (/<meta[^>]+property=["']og:image["']/i.test(html)) score += 1;

  // ğŸ“± Mobil uyum & responsive
  if (/<meta[^>]+name=["']viewport["'][^>]+width=device-width/i.test(html)) score += 2;
  if (/@media[^{]*\([^)]*max-width|min-width/i.test(html)) score += 1; // CSS media queries

  // ğŸš€ Modern teknolojiler (gÃ¼Ã§lendirilmiÅŸ)
  if (/<script[^>]+type=["']application\/ld\+json["']/i.test(html)) score += 2; // Structured data
  if (/\bwebpackJsonp\b|\b__NEXT_DATA__\b/i.test(html)) score += 2; // Modern bundler
  if (/data-reactroot|data-react-/i.test(html)) score += 2; // React
  if (/ng-version|ng-app/i.test(html)) score += 2; // Angular
  if (/\bVue\b|v-bind|v-model/i.test(html)) score += 2; // Vue.js
  if (/cdn\.jsdelivr\.net|unpkg\.com|cdnjs\.cloudflare\.com/i.test(html)) score += 1; // Modern CDN

  // ğŸ’° E-ticaret Ã¶zellikleri (bonus)
  if (/sepet|cart|basket|add to cart|sepete ekle/i.test(html)) score += 1;
  if (/Ã¶deme|payment|checkout|sipariÅŸ|order/i.test(html)) score += 1;
  if (/<script[^>]*>[^<]*gtag|google-analytics|analytics\.js/i.test(html)) score += 1; // Analytics

  // ğŸ¯ Performans gÃ¶stergeleri
  if (/<link[^>]+rel=["']preload["']/i.test(html)) score += 1; // Resource preload
  if (/<link[^>]+rel=["']dns-prefetch["']/i.test(html)) score += 1;
  if (/loading=["']lazy["']/i.test(html)) score += 1; // Lazy loading

  // âš ï¸ Kod kokularÄ± â€“ negatifler (aÄŸÄ±rlÄ±klÄ± - optimize edilmiÅŸ)
  if (/<font\b|<marquee\b|<blink\b/i.test(html)) score -= 4; // Eski HTML
  if (/<center\b/i.test(html)) score -= 2; // Deprecated tag
  
  // âš¡ HÄ±z: Sadece table varsa role kontrolÃ¼ yap
  if (/<table/i.test(html)) {
    if (/<table[^>]*border=[^>]*>/i.test(html) && !/role="table"|aria/i.test(html)) score -= 2;
  }
  
  // âš¡ HÄ±z: Sadece img varsa alt kontrolÃ¼ yap
  if (/<img/i.test(html)) {
    if (!/<img[^>]+alt=/i.test(html)) score -= 2;
  }
  
  if (/document\.write|eval\(/i.test(html)) score -= 3; // Bad practice
  
  // âš¡ HÄ±z: Yorum sayÄ±sÄ±nÄ± sadece gerektiÄŸinde hesapla
  const commentCount = (html.match(/<!--/g) || []).length;
  if (commentCount > 20) score -= 1;
  
  // ğŸ“ Ä°Ã§erik kalitesi
  if (html.length < 1000) score -= 3; // Ã‡ok az iÃ§erik
  else if (html.length < 5000) score -= 1; // Az iÃ§erik
  else if (html.length > 50000) score += 1; // Zengin iÃ§erik

  // ğŸš« KÃ¶tÃ¼ gÃ¶stergeler
  if (/error|404|not found|page not found/i.test(html) && html.length < 3000) score -= 5;
  if (/<iframe[^>]+src=["'][^"']*ad/i.test(html)) score -= 1; // Ã‡ok fazla reklam
  if (/powered by wordpress/i.test(getLowerHtml()) && !/woocommerce/i.test(getLowerHtml())) score -= 1; // Eski WP

  // ğŸ“Š Kalite kategorileri (daha dengeli)
  if (score >= 8) return 'YÃ¼ksek Kalite';
  if (score >= 3) return 'Normal Kalite';
  if (score >= 0) return 'Orta Kalite';
  return 'DÃ¼ÅŸÃ¼k Kalite';
}

